    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "PRODUCTION BUILD: InferJS-Library",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "build",
                "--prefix", "${workspaceFolder}/projects/inferjs-library/"
            ],
            "env" : { "NODE_ENV" : "production" },
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "PRODUCTION BUILD: InferJS-Compiler",
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run-script", "build",
                "--prefix", "${workspaceFolder}/projects/inferjs-compiler/"
            ],
            "env" : { "NODE_ENV" : "production" },
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TEST: INFERJS-COMPILER: All Tests",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/tests/test.mjs",
            "args": [
                "--test",
                "--no-warnings"
            ],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TEST: INFERJS-COMPILER: Parse Directory",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/tests/test-parse-dir.mjs",
            "args": [
                "--test",
                "--no-warnings"
            ],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TEST: INFERJS-COMPILER: Parse File List",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/tests/test-parse-file-list.mjs",
            "args": [
                "--test",
                "--no-warnings"
            ],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TEST: INFERJS-COMPILER: Parse File",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/tests/test-parse-file.mjs",
            "args": [
                "--test",
                "--no-warnings"
            ],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "TEST: INFERJS-COMPILER: Parse List",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/tests/test-parse-list.mjs",
            "args": [
                "--test",
                "--no-warnings"
            ],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "DEBUG: INFERJS-LIBRARY: Test1",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-library/tests/test1-esm.mjs",
            "args": [
                "--trace-warnings"
            ],
            "outputCapture": "std"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "DEBUG: INFERJS-COMPILER: Logger Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/src/core/logger.mjs",
            "args": [
                "--trace-warnings"
            ],
            "outputCapture": "std"
        },









        
        {
            "type": "node",
            "request": "launch",
            "name": "InferCompiler-Launch-Test-Action-PARSE-FILE",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/src/main.mjs",
            "args": [
                "-f",
                "--input-file=./tests/inferjs-library/test1.mjs",
                "--output-file=./tests/inferjs-library/infers/test1.infer.mjs",
                "--output-file-options-flag=wx",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "InferCompiler-Launch-Test-Action-PARSE-DIR",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/src/main.mjs",
            "args": [
                "-d",
                "--input-dir=./tests/inferjs-library",
                "--input-dir-options-recursive=true",
                "--output-file=./tests/inferjs-library/infers/test1.infer.mjs",
                "--output-file-options-flag=wx",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "InferCompiler-Launch-Test-Action-PARSE-LIST",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/src/main.mjs",
            "args": [
                "-a",
                "--input-list='./tests/inferjs-library/test1.mjs','./tests/inferjs-library/test2.mjs'",
                "--output-file=./tests/inferjs-library/infers/test1.infer.mjs",
                "--output-file-options-flag=wx",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "InferCompiler-Launch-Test-Action-PARSE-FILE-LIST",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/projects/inferjs-compiler/src/main.mjs",
            "args": [
                "-l",
                "--input-file-list=./tests/inferjs-library/list.txt",
                "--output-file=./tests/inferjs-library/infers/test1.infer.mjs",
                "--output-file-options-flag=wx",
            ]
        }
    ]
}